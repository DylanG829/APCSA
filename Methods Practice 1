class Main {
  public static void main(String[] args) {

    // String name = "Dylan Goldberg";
    // sayHello(name);
    // double length = 4;
    // double width = 5;
    // double a = area(length, width);
    // System.out.println("The area is " + a);
    int []nums = {18, 44, 22, 2, 7};
    int max = maxNum(nums);
    System.out.println("The largets value is " + max);
    int []arr = returnArray(nums);
    System.out.println(arr[0]);
    System.out.println(nums[0]);


    myName();
  }

    public static void myName(){
      System.out.println("Dylan Goldberg");
    }
    public static void sayHello(String name){
      System.out.println("Hello " + name);
    }
    public static double area(double length, double width){
      return length * width;
    }
    public static int maxNum(int []nums){
        int max = nums[0];
        for(int i = 1; i < nums.length; i++){
          if(nums[i] > max){
            max = nums[i];
          }
        }
    return max;}

    public static int[] returnArray(int[] arr){
      arr[0] = 100;
      return arr;
    }
}
